      <p class="text-center">Are you sure want to logout from this awesome system?</p>
      <p class="text-center">
        <button class="btn btn-danger md-close">Nope!</button>
        <%= link_to "Yeah, I'm sure", destroy_user_session_path, :method => :get , :class=>"btn btn-success md-close" %>
      </p>
    </div>
  </div>
</div>        <!-- Modal End -->
